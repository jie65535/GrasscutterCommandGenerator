                        <Border
                            x:Name="HoverBorder"
                            BorderBrush="{x:Null}"
                            CornerRadius="2,2,2,2"
                            Opacity="0">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0.5,0.039" EndPoint="0.5,0.976">
